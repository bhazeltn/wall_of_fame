<%= form_for @star_with_honor, :html => {:class => "form-horizontal"} do |form| %>
  <%= render :partial => '/layouts/partials/formerror', :locals => {model: @star_with_honor} %>

  <div class="form-group row">
    <%= render :partial => '/layouts/partials/select', :locals => {w: 4, lbl: "Assessment Level", fld: :star_test_id, list: StarTest.all, fld2: :name_with_level, f: form, newpath: nil} %>
    <%= render :partial => '/layouts/partials/select', :locals => {w: 4, lbl: "Skater", fld: :skater_id, list: @skaters, fld2: :name, newpath: new_skater_path, f: form} %>
  </div>
  
  <div class="form-group row">
    <%= render :partial => '/layouts/partials/date', :locals => {achieved: :achieved, f: form} %>
  </div>

  <%= render :partial => '/layouts/partials/formbtn', :locals => {cxlpath: star_with_honors_path, f: form} %>
<% end %>
