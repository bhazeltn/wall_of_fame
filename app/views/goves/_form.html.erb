<%= form_with(model: gofe, local: true) do |form| %>
  <%= render '/layouts/partials/formerror', model: gofe %>
  
  <%= render 'les', level: :level_id, event: :event_id, segment: :segment_id, f: form %>

  <div class="form-group row">
    <%= render '/layouts/partials/date', achieved: :achieved, f: form %>
    <%= render '/layouts/partials/select', w: 3, lbl: "Competition", fld: :competition_id, fld2: :name, list: @competitions, newpath: new_competition_path, f: form %>
  </div>  

  <div class="form-group row">
    <%= render '/layouts/partials/select', w: 3, lbl: "Skater", fld: :skater1, fld2: :name, list: @skaters, newpath: new_skater_path, f: form %>
    <%= render '/layouts/partials/select', w: 3, lbl: "Skater", fld: :skater2, fld2: :name, list: @skaters, newpath: new_skater_path, f: form %>
  </div>
 
  <div class="form-group row">
    <%= render '/layouts/partials/textfield', lbl: "Element", fld: :element, fld2: :gofe_element, f: form %>
    <%= render '/layouts/partials/goe', grade: :grade, f: form %>
  </div> 

  <%= render '/layouts/partials/formbtn', cxlpath: goves_path, f: form %>

<% end %>
