<%= form_with(model: club_award, local: true) do |form| %>
  <%= render :partial => '/layouts/partials/formerror', :locals => {model: club_award} %>

  <div class="form-group row">
    <%= render :partial => '/layouts/partials/select', :locals => {w: 4, lbl: "Skater", fld: :skater_id, fld2: :name, list: @skaters, newpath: new_skater_path, f: form} %>
    <%= form.label :award_id, "Award", class: "col-form-label col-md-1" %>
    <div class="col-md-3">
      <%= form.grouped_collection_select :award_id, award_types, :award, :name, :id, :name, {:include_blank => true}, {class: "custom-select"} %>
    </div>
  </div>

  <div class="form-group row">
    <%= render :partial => '/layouts/partials/year', :locals => {yr: :year, f: form} %>
  </div>

   <%= render :partial => '/layouts/partials/formbtn', :locals => {cxlpath: club_awards_path, f: form} %>
<% end %>
