<%= form_for @club_award, :html => {:class => "form-horizontal"} do |form| %>
  <%= render :partial => '/layouts/partials/formerror', :locals => {model: @club_award} %>

  <div class="form-group row">
    <%= render :partial => '/layouts/partials/select', :locals => {w: 4, lbl: "Skater", fld: :skater_id, fld2: :name, list: Skater.all, newpath: new_skater_path, f: form} %>
    <%= render :partial => '/layouts/partials/select', :locals => {w: 4, lbl: "Award", fld: :award_id, fld2: :name, list: Award.all, newpath: nil, f: form} %>
  </div>

  <div class="form-group row">
    <%= render :partial => '/layouts/partials/year', :locals => {yr: :year, f: form} %>
  </div>

   <%= render :partial => '/layouts/partials/formbtn', :locals => {cxlpath: club_awards_path, f: form} %>
<% end %>
